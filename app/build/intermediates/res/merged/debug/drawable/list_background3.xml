<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">

    <solid android:color="#b59206" />

    <padding
        android:left="10dp"
        android:top="10dp"
        android:right="10dp"
        android:bottom="0dp"></padding>

    <corners
        android:topLeftRadius="10dp"
        android:topRightRadius="@dimen/top_right_radius"
        android:bottomRightRadius="@dimen/bottom_right_radius"
        android:bottomLeftRadius="10dp" />


    <gradient
        android:endColor="#b59206"
        android:startColor="#b59206"
        android:angle="45" />

</shape>